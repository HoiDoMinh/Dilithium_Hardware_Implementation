`timescale 1ns/1ps
`include "byte_and_print.vh"

module tb_polyz_pack;

  // a_in nên là wire n?u dùng assign
  wire [8191:0] a_in;
  wire [5119:0] r_out;
integer i;
  assign a_in = 8192';

  polyz_pack dut (
    .a_in(a_in),
    .r_out(r_out)
  );

  wire [7:0] r_out_t [0:639];
  `BYTE_ASSIGN_GEN(r, 640, r_out, r_out_t)

  initial begin
    #1;
    `PRINT_HEX_ARRAY("r out", r_out_t, 640)
    $finish;
  end

endmodule

